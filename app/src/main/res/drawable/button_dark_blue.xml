<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="0dip" />
            <stroke android:width="1dip" android:color="@color/button_dark_blue_pressed" />
            <solid android:color="@color/button_dark_blue_pressed"></solid>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="0dip" />
            <stroke android:width="1dip" android:color="@color/button_dark_blue" />
            <solid android:color="@color/button_dark_blue"></solid>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="0dip" />
            <stroke android:width="1dip" android:color="@color/button_dark_blue" />
            <solid android:color="@color/button_dark_blue"></solid>
        </shape>
    </item>

</selector>